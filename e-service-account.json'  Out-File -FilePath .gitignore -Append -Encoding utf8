[1mdiff --git a/src/app/auth/gql-auth.guard.ts b/src/app/auth/gql-auth.guard.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..681f5b8[m
[1m--- /dev/null[m
[1m+++ b/src/app/auth/gql-auth.guard.ts[m
[36m@@ -0,0 +1,40 @@[m
[32m+[m[32m// src/app/auth/gql-auth.guard.ts[m
[32m+[m[32mimport {[m
[32m+[m[32m  Injectable,[m
[32m+[m[32m  CanActivate,[m
[32m+[m[32m  ExecutionContext,[m
[32m+[m[32m  Inject,[m
[32m+[m[32m} from '@nestjs/common';[m
[32m+[m[32mimport { GqlExecutionContext } from '@nestjs/graphql';[m
[32m+[m[32mimport * as admin from 'firebase-admin';[m
[32m+[m[32mimport { FIREBASE_ADMIN } from '../core/firebase/firebase-admin.provider';[m
[32m+[m[32mimport { PrismaService } from 'prisma/prisma.service';[m
[32m+[m
[32m+[m[32m@Injectable()[m
[32m+[m[32mexport class GqlAuthGuard implements CanActivate {[m
[32m+[m[32m  constructor([m
[32m+[m[32m    private readonly prisma: PrismaService,[m
[32m+[m[32m    @Inject(FIREBASE_ADMIN) private readonly fb: typeof admin,[m
[32m+[m[32m  ) { }[m
[32m+[m
[32m+[m[32m  async canActivate(ctx: ExecutionContext): Promise<boolean> {[m
[32m+[m[32m    const gqlCtx = GqlExecutionContext.create(ctx);[m
[32m+[m[32m    const req = gqlCtx.getContext().req as {[m
[32m+[m[32m      headers?: Record<string, string>;[m
[32m+[m[32m      user?: unknown;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const auth = req.headers?.authorization ?? '';[m
[32m+[m[32m    const token = auth.startsWith('Bearer ') ? auth.slice(7) : null;[m
[32m+[m[32m    if (!token) return false;[m
[32m+[m
[32m+[m[32m    const decoded = await this.fb.auth().verifyIdToken(token);[m
[32m+[m[32m    const user = await this.prisma.user.findFirst({[m
[32m+[m[32m      where: { firebaseUid: decoded.uid, archived: false },[m
[32m+[m[32m    });[m
[32m+[m[32m    if (!user) return false;[m
[32m+[m
[32m+[m[32m    req.user = user;[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/src/app/auth/roles.decorator.ts b/src/app/auth/roles.decorator.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..3e3861f[m
[1m--- /dev/null[m
[1m+++ b/src/app/auth/roles.decorator.ts[m
[36m@@ -0,0 +1,3 @@[m
[32m+[m[32mimport { SetMetadata } from '@nestjs/common';[m
[32m+[m[32mexport const ROLES_KEY = 'roles';[m
[32m+[m[32mexport const Roles = (...roles: string[]) => SetMetadata(ROLES_KEY, roles);[m
[1mdiff --git a/src/app/auth/roles.guard.ts b/src/app/auth/roles.guard.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..212622c[m
[1m--- /dev/null[m
[1m+++ b/src/app/auth/roles.guard.ts[m
[36m@@ -0,0 +1,20 @@[m
[32m+[m[32mimport { CanActivate, ExecutionContext, Injectable } from '@nestjs/common';[m
[32m+[m[32mimport { Reflector } from '@nestjs/core';[m
[32m+[m[32mimport { GqlExecutionContext } from '@nestjs/graphql';[m
[32m+[m[32mimport { ROLES_KEY } from './roles.decorator';[m
[32m+[m
[32m+[m[32m@Injectable()[m
[32m+[m[32mexport class RolesGuard implements CanActivate {[m
[32m+[m[32m  constructor(private reflector: Reflector) { }[m
[32m+[m[32m  canActivate(ctx: ExecutionContext): boolean {[m
[32m+[m[32m    const roles = this.reflector.getAllAndOverride<string[]>(ROLES_KEY, [[m
[32m+[m[32m      ctx.getHandler(),[m
[32m+[m[32m      ctx.getClass(),[m
[32m+[m[32m    ]);[m
[32m+[m[32m    if (!roles || roles.length === 0) return true;[m
[32m+[m
[32m+[m[32m    const req = GqlExecutionContext.create(ctx).getContext().req;[m
[32m+[m[32m    const user = req.user;[m
[32m+[m[32m    return !!user && roles.includes(user.role);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
