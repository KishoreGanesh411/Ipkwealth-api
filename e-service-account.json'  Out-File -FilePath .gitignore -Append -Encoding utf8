[32m+[m[32mimport { Provider } from '@nestjs/common';[m
[32m+[m[32mimport * as admin from 'firebase-admin';[m
[32m+[m[32mimport * as serviceAccount from '../../../../firebase-service-account.json';[m
[32m+[m
[32m+[m[32mexport const FIREBASE_ADMIN = 'FIREBASE_ADMIN';[m
[32m+[m
[32m+[m[32mexport const FirebaseAdminProvider: Provider = {[m
[32m+[m[32m  provide: FIREBASE_ADMIN,[m
[32m+[m[32m  useFactory: () => {[m
[32m+[m[32m    if (admin.apps.length === 0) {[m
[32m+[m[32m      admin.initializeApp({[m
[32m+[m[32m        credential: admin.credential.cert([m
[32m+[m[32m          serviceAccount as admin.ServiceAccount,[m
[32m+[m[32m        ),[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m    return admin;[m
[32m+[m[32m  },[m
[32m+[m[32m};[m
