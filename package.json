  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postinstall": "npm run prisma:db:push && npm run prisma:generate",
    "prisma:db:push": "npx prisma db push --schema prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema prisma/schema.prisma",
    "api": "npm run prisma:db:push && npm run dev"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@nestjs/apollo": "^13.1.0",
