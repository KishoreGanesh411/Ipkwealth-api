    "luxon": "^3.7.1",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
